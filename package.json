      }
    ]
  },
  "output": "${package.dir}/build",
  "local": true,
  "requires": [
    "GeoExt"
  ],
  "scripts": {},
  "dependencies": {},
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "phantomjs": "^1.9.19"
  }
}
